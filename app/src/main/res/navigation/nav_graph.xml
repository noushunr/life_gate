<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/splashFragment">

    <fragment
        android:id="@+id/splashFragment"
        android:name="com.lifegate.app.ui.fragments.splash.SplashFragment"
        android:label="splash_fragment"
        tools:layout="@layout/splash_fragment" >
        <action
            android:id="@+id/action_splashFragment_to_chooseLocationFragment"
            app:destination="@id/chooseLocationFragment"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_splashFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/splashFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/chooseLocationFragment"
        android:name="com.lifegate.app.ui.fragments.choose.ChooseLocationFragment"
        android:label="choose_location_fragment"
        tools:layout="@layout/choose_location_fragment" >
        <action
            android:id="@+id/action_chooseLocationFragment_to_onBoardFragment"
            app:destination="@id/onBoardFragment"
            app:popUpTo="@id/chooseLocationFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.lifegate.app.ui.fragments.auth.login.LoginFragment"
        android:label="login_fragment"
        tools:layout="@layout/login_fragment" >
        <action
            android:id="@+id/action_loginFragment_to_signUpFragment"
            app:destination="@id/signUpFragment" />
        <action
            android:id="@+id/action_loginFragment_to_myPlanFragment"
            app:destination="@id/myPlanFragment"
            app:popUpTo="@id/homeFragment"/>
    </fragment>
    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.lifegate.app.ui.fragments.auth.signup.SignUpFragment"
        android:label="sign_up_fragment"
        tools:layout="@layout/sign_up_fragment" >
        <action
            android:id="@+id/action_signUpFragment_to_myPlanFragment"
            app:destination="@id/myPlanFragment"
            app:popUpTo="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.lifegate.app.ui.fragments.home.HomeFragment"
        android:label="home_fragment"
        tools:layout="@layout/home_fragment" >
        <action
            android:id="@+id/action_homeFragment_to_featuredCoachFragment"
            app:destination="@id/coachesFragment" />
        <action
            android:id="@+id/action_homeFragment_to_featuredPlanFragment"
            app:destination="@id/featuredPlanFragment" />
        <action
            android:id="@+id/action_homeFragment_to_coachDetailFragment"
            app:destination="@id/coachDetailFragment" />
        <action
            android:id="@+id/action_homeFragment_to_planDetailFragment"
            app:destination="@id/planDetailFragment" />
        <action
            android:id="@+id/action_homeFragment_to_videoActivity"
            app:destination="@id/videoActivity" />
        <action
            android:id="@+id/action_homeFragment_to_sliderActivity"
            app:destination="@id/sliderActivity" />
        <action
            android:id="@+id/action_homeFragment_to_dietPlanFragment"
            app:destination="@id/dietPlanFragment" />
        <action
            android:id="@+id/action_homeFragment_to_workoutPlanFragment"
            app:destination="@id/workoutPlanFragment" />
    </fragment>
    <fragment
        android:id="@+id/coachesFragment"
        android:name="com.lifegate.app.ui.fragments.coach.list.CoachesFragment"
        android:label="coaches_fragment"
        tools:layout="@layout/coaches_fragment" >
        <action
            android:id="@+id/action_coachesFragment_to_coachDetailFragment"
            app:destination="@id/coachDetailFragment" />
        <action
            android:id="@+id/action_coachesFragment_to_sliderActivity"
            app:destination="@id/sliderActivity" />
    </fragment>
    <fragment
        android:id="@+id/caloriesFragment"
        android:name="com.lifegate.app.ui.fragments.calories.CaloriesFragment"
        android:label="calories_fragment"
        tools:layout="@layout/calories_fragment" >
        <action
            android:id="@+id/action_caloriesFragment_to_historyFragment"
            app:destination="@id/historyFragment" />
    </fragment>
    <fragment
        android:id="@+id/myPlanFragment"
        android:name="com.lifegate.app.ui.fragments.myplan.MyPlanFragment"
        android:label="my_plan_fragment"
        tools:layout="@layout/my_plan_fragment" >
        <action
            android:id="@+id/action_myPlanFragment_to_historyFragment"
            app:destination="@id/historyFragment" />
        <action
            android:id="@+id/action_myPlanFragment_to_workoutPlanFragment"
            app:destination="@id/workoutPlanFragment" />
        <action
            android:id="@+id/action_myPlanFragment_to_dietPlanFragment"
            app:destination="@id/dietPlanFragment" />
    </fragment>
    <fragment
        android:id="@+id/dietPlanFragment"
        android:name="com.lifegate.app.ui.fragments.diet.DietPlanFragment"
        android:label="diet_plan_fragment"
        tools:layout="@layout/diet_plan_fragment" >
        <argument
            android:name="planId"
            app:argType="string"
            app:nullable="false"
            android:defaultValue=""/>
        <argument
            android:name="purchaseId"
            app:argType="string"
            app:nullable="false"
            android:defaultValue="" />
        <action
            android:id="@+id/action_dietPlanFragment_to_nutritionDetailFragment"
            app:destination="@id/nutritionDetailFragment" />
        <action
            android:id="@+id/action_dietPlanFragment_to_restaurantFragment"
            app:destination="@id/restaurantFragment" />
        <action
            android:id="@+id/action_dietPlanFragment_to_nutritionRestaurantFragment"
            app:destination="@id/nutritionRestaurantFragment" />
        <action
            android:id="@+id/action_dietPlanFragment_to_nutritionGroceryFragment"
            app:destination="@id/nutritionGroceryFragment" />
        <action
            android:id="@+id/action_dietPlanFragment_to_sliderActivity"
            app:destination="@id/sliderActivity" />
    </fragment>
    <action android:id="@+id/action_global_loginFragment" app:destination="@id/loginFragment" />
    <fragment
        android:id="@+id/coachRegisterFragment"
        android:name="com.lifegate.app.ui.fragments.coach.register.CoachRegisterFragment"
        android:label="coach_register_fragment"
        tools:layout="@layout/coach_register_fragment" />
    <action android:id="@+id/action_global_coachRegisterFragment" app:destination="@id/coachRegisterFragment" />
    <fragment
        android:id="@+id/onBoardFragment"
        android:name="com.lifegate.app.ui.fragments.onboard.OnBoardFragment"
        android:label="on_board_fragment"
        tools:layout="@layout/on_board_fragment" >
        <action
            android:id="@+id/action_onBoardFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/onBoardFragment"
            app:popUpToInclusive="true"  />
    </fragment>
    <fragment
        android:id="@+id/restaurantFragment"
        android:name="com.lifegate.app.ui.fragments.restaurant.RestaurantFragment"
        android:label="restaurant_fragment"
        tools:layout="@layout/restaurant_fragment" />
    <action
        android:id="@+id/action_global_restaurantFragment"
        app:destination="@id/restaurantFragment"/>
    <fragment
        android:id="@+id/historyFragment"
        android:name="com.lifegate.app.ui.fragments.history.HistoryFragment"
        android:label="history_fragment"
        tools:layout="@layout/history_fragment" >
        <action
            android:id="@+id/action_historyFragment_to_workoutHistoryFragment"
            app:destination="@id/workoutHistoryFragment" />
        <action
            android:id="@+id/action_historyFragment_to_nutritionDetailFragment"
            app:destination="@id/nutritionDetailFragment" />
        <action
            android:id="@+id/action_historyFragment_to_workoutDetailFragment"
            app:destination="@id/workoutDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/nutritionDetailFragment"
        android:name="com.lifegate.app.ui.fragments.nutrition.NutritionDetailFragment"
        android:label="nutrition_detail_fragment"
        tools:layout="@layout/nutrition_detail_fragment" >
        <argument
            android:name="planId"
            app:argType="string"
            app:nullable="false"
            android:defaultValue=""/>
        <argument
            android:name="purchaseId"
            app:argType="string"
            app:nullable="false"
            android:defaultValue="" />
        <argument
            android:name="date"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null" />
        <action
            android:id="@+id/action_nutritionDetailFragment_to_sliderActivity"
            app:destination="@id/sliderActivity" />
    </fragment>
    <fragment
        android:id="@+id/workoutHistoryFragment"
        android:name="com.lifegate.app.ui.fragments.workout.WorkoutHistoryFragment"
        android:label="workout_history_fragment"
        tools:layout="@layout/workout_history_fragment" >
        <argument
            android:name="planId"
            app:argType="string"
            app:nullable="false"
            android:defaultValue=""/>
        <argument
            android:name="purchaseId"
            app:argType="string"
            app:nullable="false"
            android:defaultValue="" />
        <action
            android:id="@+id/action_workoutHistoryFragment_to_workoutDetailFragment"
            app:destination="@id/workoutDetailFragment" />
        <action
            android:id="@+id/action_workoutHistoryFragment_to_sliderActivity"
            app:destination="@id/sliderActivity" />
    </fragment>
    <fragment
        android:id="@+id/workoutPlanFragment"
        android:name="com.lifegate.app.ui.fragments.workout.plan.WorkoutPlanFragment"
        android:label="workout_plan_fragment"
        tools:layout="@layout/workout_plan_fragment" >
        <argument
            android:name="planId"
            app:argType="string"
            app:nullable="false"
            android:defaultValue=""/>
        <argument
            android:name="purchaseId"
            app:argType="string"
            app:nullable="false"
            android:defaultValue="" />
        <action
            android:id="@+id/action_workoutPlanFragment_to_historyFragment"
            app:destination="@id/historyFragment" />
        <action
            android:id="@+id/action_workoutPlanFragment_to_workoutHistoryFragment"
            app:destination="@id/workoutHistoryFragment" />
        <action
            android:id="@+id/action_workoutPlanFragment_to_flowChartFragment"
            app:destination="@id/flowChartFragment" />
        <action
            android:id="@+id/action_workoutPlanFragment_to_sliderActivity"
            app:destination="@id/sliderActivity" />
    </fragment>
    <fragment
        android:id="@+id/coachDetailFragment"
        android:name="com.lifegate.app.ui.fragments.coach.detail.CoachDetailFragment"
        android:label="coach_detail_fragment"
        tools:layout="@layout/coach_detail_fragment" >
        <argument
            android:name="coachId"
            app:argType="string"
            app:nullable="false" />
        <action
            android:id="@+id/action_coachDetailFragment_to_planDetailFragment"
            app:destination="@id/planDetailFragment" />
        <action
            android:id="@+id/action_coachDetailFragment_to_sliderActivity"
            app:destination="@id/sliderActivity" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.lifegate.app.ui.fragments.profile.ProfileFragment"
        android:label="profile_fragment"
        tools:layout="@layout/profile_fragment" >
        <action
            android:id="@+id/action_profileFragment_to_sliderActivity"
            app:destination="@id/sliderActivity" />
    </fragment>
    <action android:id="@+id/action_global_profileFragment" app:destination="@id/profileFragment"/>
    <fragment
        android:id="@+id/planDetailFragment"
        android:name="com.lifegate.app.ui.fragments.myplan.detail.PlanDetailFragment"
        android:label="plan_detail_fragment"
        tools:layout="@layout/plan_detail_fragment" >
        <argument
            android:name="planId"
            app:argType="string"
            app:nullable="false" />
        <action
            android:id="@+id/action_planDetailFragment_to_checkoutFragment"
            app:destination="@id/checkoutFragment" />
        <action
            android:id="@+id/action_planDetailFragment_to_sliderActivity"
            app:destination="@id/sliderActivity" />
    </fragment>
    <fragment
        android:id="@+id/checkoutFragment"
        android:name="com.lifegate.app.ui.fragments.checkout.CheckoutFragment"
        android:label="checkout_fragment"
        tools:layout="@layout/checkout_fragment" >
        <argument
            android:name="plan"
            app:argType="string"
            app:nullable="false" />
        <argument
            android:name="name"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null"/>
        <argument
            android:name="pic"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null"/>
        <argument
            android:name="price"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null"/>
        <argument
            android:name="extra"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null"/>
        <argument
            android:name="total_price"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null"/>
        <argument
            android:name="duration"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null"/>
        <action
            android:id="@+id/action_checkoutFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/featuredCoachFragment"
        android:name="com.lifegate.app.ui.fragments.home.coach.FeaturedCoachFragment"
        android:label="Featured Coach" >
        <argument
            android:name="json"
            app:argType="string"
            app:nullable="false" />
        <action
            android:id="@+id/action_featuredCoachFragment_to_coachDetailFragment"
            app:destination="@id/coachDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/featuredPlanFragment"
        android:name="com.lifegate.app.ui.fragments.home.plan.FeaturedPlanFragment"
        android:label="Featured Plan"
        tools:layout="@layout/featured_plan_fragment" >
        <argument
            android:name="json"
            app:argType="string"
            app:nullable="false" />
        <action
            android:id="@+id/action_featuredPlanFragment_to_planDetailFragment"
            app:destination="@id/planDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/nutritionRestaurantFragment"
        android:name="com.lifegate.app.ui.fragments.restaurant.nutrition.NutritionRestaurantFragment"
        android:label="nutrition_restaurant_fragment"
        tools:layout="@layout/nutrition_restaurant_fragment" >
        <argument
            android:name="planId"
            app:argType="string"
            app:nullable="false"
            android:defaultValue=""/>
        <argument
            android:name="purchaseId"
            app:argType="string"
            app:nullable="false"
            android:defaultValue="" />
    </fragment>
    <fragment
        android:id="@+id/nutritionGroceryFragment"
        android:name="com.lifegate.app.ui.fragments.grocery.NutritionGroceryFragment"
        android:label="nutrition_grocery_fragment"
        tools:layout="@layout/nutrition_grocery_fragment" >
        <argument
            android:name="planId"
            app:argType="string"
            app:nullable="false"
            android:defaultValue=""/>
        <argument
            android:name="purchaseId"
            app:argType="string"
            app:nullable="false"
            android:defaultValue="" />
    </fragment>
    <fragment
        android:id="@+id/workoutDetailFragment"
        android:name="com.lifegate.app.ui.fragments.workout.detail.WorkoutDetailFragment"
        android:label="workout_detail_fragment"
        tools:layout="@layout/workout_detail_fragment" >
        <argument
            android:name="planId"
            app:argType="string"
            app:nullable="false"
            android:defaultValue=""/>
        <argument
            android:name="purchaseId"
            app:argType="string"
            app:nullable="false"
            android:defaultValue="" />
        <argument
            android:name="date"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null" />
        <action
            android:id="@+id/action_workoutDetailFragment_to_sliderActivity"
            app:destination="@id/sliderActivity" />
    </fragment>
    <activity
        android:id="@+id/videoActivity"
        android:name="com.lifegate.app.ui.activity.VideoActivity"
        android:label="activity_video"
        tools:layout="@layout/activity_video" >
        <argument
            android:name="url"
            app:argType="string"
            app:nullable="false"
            android:defaultValue="" />
    </activity>
    <fragment
        android:id="@+id/notificationFragment"
        android:name="com.lifegate.app.ui.fragments.notification.NotificationFragment"
        android:label="notification_fragment"
        tools:layout="@layout/notification_fragment" />
    <action android:id="@+id/action_global_notificationFragment" app:destination="@id/notificationFragment" />
    <fragment
        android:id="@+id/dietListFragment"
        android:name="com.lifegate.app.ui.fragments.diet.list.DietListFragment"
        android:label="diet_list_fragment"
        tools:layout="@layout/diet_list_fragment" >
        <action
            android:id="@+id/action_dietListFragment_to_dietPlanFragment"
            app:destination="@id/dietPlanFragment" />
        <action
            android:id="@+id/action_dietListFragment_to_workoutPlanFragment"
            app:destination="@id/workoutPlanFragment" />
    </fragment>
    <fragment
        android:id="@+id/flowChartFragment"
        android:name="com.lifegate.app.ui.fragments.workout.flow.FlowChartFragment"
        android:label=""
        tools:layout="@layout/flow_chart_fragment" >
        <argument
            android:name="planId"
            app:argType="string"
            app:nullable="false"
            android:defaultValue=""/>
        <argument
            android:name="purchaseId"
            app:argType="string"
            app:nullable="false"
            android:defaultValue="" />
    </fragment>
    <fragment
        android:id="@+id/coachMessageFragment"
        android:name="com.lifegate.app.ui.fragments.message.CoachMessageFragment"
        android:label=" "
        tools:layout="@layout/coach_message_fragment" />
    <action android:id="@+id/action_global_coachMessageFragment" app:destination="@id/coachMessageFragment"/>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.lifegate.app.ui.fragments.settings.SettingsFragment"
        android:label=" "
        tools:layout="@layout/settings_fragment" >
        <action
            android:id="@+id/action_settingsFragment_to_coachRegisterFragment"
            app:destination="@id/coachRegisterFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_policyFragment"
            app:destination="@id/policyFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="true"/>
    </fragment><action android:id="@+id/action_global_settingsFragment" app:destination="@id/settingsFragment"/>
    <fragment
        android:id="@+id/policyFragment"
        android:name="com.lifegate.app.ui.fragments.pricy_policy.PolicyFragment"
        android:label=" "
        tools:layout="@layout/policy_fragment" />
    <activity
        android:id="@+id/sliderActivity"
        android:name="com.lifegate.app.ui.activity.SliderActivity"
        android:label="SliderActivity" >
        <argument
            android:name="json"
            app:argType="string"
            app:nullable="false"
            android:defaultValue="" />
    </activity>
</navigation>